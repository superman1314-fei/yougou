.search {
  padding: 20rpx;
  background: #ff2d4a;
}

.search_content {
  height: 61rpx;
  width: 100%;
  background: #fff;
  border-radius: 4px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_content image{
    height: 33rpx;
    width: 30rpx;
    color: #bcbbbc;
    padding-right: 13rpx;
}
.search_content text{
  font-size: 22rpx;
    color: #bcbbbc;
}
.page_swiper{
  height: 340rpx;
}
.page_swiper image{
   height: 340rpx;
    width: 100%;
}
.menus{

  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 20rpx 0;
}
.menus image{
  width: 128rpx;
  height: 133rpx;
}